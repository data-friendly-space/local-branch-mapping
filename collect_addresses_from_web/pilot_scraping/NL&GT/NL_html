    // Vul hierboven (tussen de haakjes en NA de punt de UNIEKE CLASS in met daarin het page of post id van nieuwsbericht + campagnepagina (die vind je in de broncode)
	var banner = jQuery("#menu-item-27478 a"); // Niet wijzigen, dit selecteert de donatieknop in het menu
	var newUrl = "https://doneer.rodekruis.nl/cycloon-idai"; // Vul hier (tussen de haakjes) de nieuwe URL in
	jQuery(banner).attr("href", newUrl); // Niet wijzigen, dit stelt de nieuwe URL in
    }
});
