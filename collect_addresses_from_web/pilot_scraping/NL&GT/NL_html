// Bij het zoeken naar de dichtstbijzijnde vestiging werd nog niet ingezoomd op 
// desbetreffende vestiging. Met deze code is dat verholpen.
jQuery(document).ready(function() {
    if (jQuery('.page-id-15308').length > 0 || jQuery('.page-id-18482').length > 0 || jQuery('.page-id-18828').length >0) {
        jQuery('#storeLocator__searchBar').keypress(function(event) {
    		var keycode = (event.keyCode ? event.keyCode : event.which);
    		if (keycode == '13') {
            	if (jQuery('#storeLocator__searchBar').val() != "") {
                  if(jQuery('#store0').length > 0) {
                    setTimeout(function() {
                        jQuery('#store0').click();
                    }, 1000);
                  }
                }
            }
        });
      
      	jQuery('body').on('mousedown', '.pac-item', function() {
            if ( jQuery('#store0').length > 0 ) {
                setTimeout(function() {
                    jQuery('#store0').click();
                }, 2000);
            }
        });
    }
});

// Bij klik op locatie icoon op contactpagina, zal de dichtstbijzijnde vestiging
// getoond worden bij jouw huidige locatie.
jQuery(document).ready(function() {
    if (jQuery('.page-id-15308').length > 0 || jQuery('.page-id-18482').length > 0 || jQuery('.page-id-18828').length > 0) {
        jQuery('#geolocator').click(function() {
        	if(jQuery('#store0').length > 0) {
          		setTimeout(function() {
            		jQuery('#store0').click();
          		}, 5000);
        	}
        });
    }
});





// Bij formulier Bedankt voor je stem zorgen dat de anchor wordt
// opgeslagen in een veldje zodat het meegestuurd wordt in formulier.
jQuery(document).ready(function() {
    if (jQuery('.page-id-103165').length > 0) {
	var sentiment = window.location.hash.substr(1);
	if(sentiment.match(/positie/)) {
		var sentiment = "Positief sentiment";
	}else 
   	if(sentiment.match(/negatie/)) {
		var sentiment = "Negatief sentiment";
	}else{
      	var sentiment = "Reactietype niet gemeten";
    }
	jQuery( '#input_121_11' ).val( sentiment );
    }
});




// Doe Mee pagina
jQuery(document).ready(function() {
    if (jQuery('.page-id-15305').length > 0) {
	jQuery('.button-geld-geven').click(function () {
		if (jQuery("#doe-mee-tijd").hasClass("doe-mee-actief")) {
    		jQuery('#doe-mee-tijd').removeClass('doe-mee-actief');
    		jQuery('#doe-mee-tijd').addClass('doe-mee-inactief');
		}
		if (jQuery("#doe-mee-onderwijs").hasClass("doe-mee-actief")) {
    		jQuery('#doe-mee-onderwijs').removeClass('doe-mee-actief');
    		jQuery('#doe-mee-onderwijs').addClass('doe-mee-inactief');
		}
		if (jQuery("#doe-mee-geld").hasClass("doe-mee-inactief")) {
    		jQuery('#doe-mee-geld').removeClass('doe-mee-inactief');
    		jQuery('#doe-mee-geld').addClass('doe-mee-actief');
		}
		if (jQuery("#doe-mee-uitgelicht").hasClass("doe-mee-actief")) {
    		jQuery('#doe-mee-uitgelicht').removeClass('doe-mee-actief');
    		jQuery('#doe-mee-uitgelicht').addClass('doe-mee-inactief');
		}
		jQuery(window).trigger('resize');
	});

	jQuery('.button-tijd-geven').click(function () {
		if (jQuery("#doe-mee-tijd").hasClass("doe-mee-inactief")) {
    		jQuery('#doe-mee-tijd').removeClass('doe-mee-inactief');
    		jQuery('#doe-mee-tijd').addClass('doe-mee-actief');
		}
		if (jQuery("#doe-mee-onderwijs").hasClass("doe-mee-actief")) {
    		jQuery('#doe-mee-onderwijs').removeClass('doe-mee-actief');
    		jQuery('#doe-mee-onderwijs').addClass('doe-mee-inactief');
		}
		if (jQuery("#doe-mee-geld").hasClass("doe-mee-actief")) {
    		jQuery('#doe-mee-geld').removeClass('doe-mee-actief');
    		jQuery('#doe-mee-geld').addClass('doe-mee-inactief');
		}
		if (jQuery("#doe-mee-uitgelicht").hasClass("doe-mee-actief")) {
    		jQuery('#doe-mee-uitgelicht').removeClass('doe-mee-actief');
    		jQuery('#doe-mee-uitgelicht').addClass('doe-mee-inactief');
		}
		jQuery(window).trigger('resize');
	});

	jQuery('.button-onderwijs').click(function () {
		if (jQuery("#doe-mee-tijd").hasClass("doe-mee-actief")) {
    		jQuery('#doe-mee-tijd').removeClass('doe-mee-actief');
    		jQuery('#doe-mee-tijd').addClass('doe-mee-inactief');
		}
		if (jQuery("#doe-mee-onderwijs").hasClass("doe-mee-inactief")) {
    		jQuery('#doe-mee-onderwijs').removeClass('doe-mee-inactief');
    		jQuery('#doe-mee-onderwijs').addClass('doe-mee-actief');
		}
		if (jQuery("#doe-mee-geld").hasClass("doe-mee-actief")) {
    		jQuery('#doe-mee-geld').removeClass('doe-mee-actief');
    		jQuery('#doe-mee-geld').addClass('doe-mee-inactief');
		}
		if (jQuery("#doe-mee-uitgelicht").hasClass("doe-mee-actief")) {
    		jQuery('#doe-mee-uitgelicht').removeClass('doe-mee-actief');
    		jQuery('#doe-mee-uitgelicht').addClass('doe-mee-inactief');
		}
		jQuery(window).trigger('resize');
	});
    }
});




// Formulier alleen tonen wanneer er op een linkje geklikt wordt
// op de evenementenhulp aanvraag pagina.
jQuery(document).ready(function() {
    if (jQuery('.page-id-18828').length > 0) {
	jQuery('.trigger-content a').click(function() {		
		jQuery('#aanvraagformulier').toggleClass('formulierHide');
		jQuery('html, body').animate({
    			scrollTop: jQuery("#aanvraagformulier").offset().top
		}, 10);
	});
    }
});




// Bij EHV op je evenement pagina zorgen dat wanneer hashtag wordt meegegeven
// het formulier openklapt.
jQuery(document).ready(function() {
    if (jQuery('.page-id-18828').length > 0) {
	var hashtag = window.location.hash.substr(1);
	if(hashtag.match(/aanvraag/)) {	
		jQuery('#aanvraagformulier').toggleClass('formulierHide');
		jQuery('html, body').animate({
    			scrollTop: jQuery("#aanvraagformulier").offset().top
		}, 10);
	}
    }
});

// Periodiek schenken formulier: maak tekst rood als e-mailadres partner + schenker hetzelfde is en schakel verzendbutton uit
jQuery(document).ready(function() { 
	if (jQuery('.page-id-97543').length > 0) {
		jQuery("#input_104_28").change(function() {
					var emailschenker = jQuery("#input_104_9").val(); 
					var emailpartner = jQuery("#input_104_28").val();
					if(emailschenker == emailpartner) {
			        	jQuery("#input_104_28").css("color", "red");
						jQuery("#gfield_description_104_28").css("color","red");
						jQuery("#gform_submit_button_104").css("display","none");
                      	alert("Het e-mailadres van uw partner mag niet hetzelfde zijn als uw eigen e-mailadres.");
					}else{
			        	jQuery("#input_104_28").css("color", "#222222");
						jQuery("#gfield_description_104_28").css("color","#222222");
						jQuery("#gform_submit_button_104").css("display","inline-block");
					}
		});
	}
}); 






