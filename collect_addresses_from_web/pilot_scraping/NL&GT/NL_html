// Petitie EHBO in het onderwijs | Postcode check
jQuery(document).ready(function() { 
	if (jQuery('.html_entry_id_106240').length > 0 || jQuery('.html_entry_id_106521').length > 0 || jQuery('.html_entry_id_106693').length > 0) {
		jQuery("#input_132_2_5").change(function() {
					var postcode = jQuery("#input_132_2_5").val(); 
					var matches = postcode.match(/\d+/g);
                    if (matches != null) {
                        alert('Het lijkt erop alsof je een postcode hebt ingevuld in het Woonplaats-veld. Kun je dit controleren?');
                    }
		});
	}
}); 
