	if (jQuery('.html_entry_id_106240').length > 0 || jQuery('.html_entry_id_106521').length > 0 || jQuery('.html_entry_id_106693').length > 0) {
		jQuery("#input_132_3").change(function() {
					var postcode = jQuery("#input_132_3").val(); 
					var matches = postcode.match(/2019/g);
                    if (matches != null) {
                        alert('Het lijkt erop alsof je het huidige jaartal als geboortedatum hebt ingevuld. Kun je dit controleren?');
                    }
		});
	}
}); 


